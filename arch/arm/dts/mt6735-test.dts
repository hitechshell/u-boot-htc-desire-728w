/dts-v1/;
#include "mt6735.dtsi"
#include "mt6735-u-boot.dtsi"

/ {
    #address-cells = <1>;
    #size-cells = <1>;
	model = "MT6735";
	compatible = "mediatek,test", "mediatek,mt6735";

	chosen {
		stdout-path = &uart0;
		};
        
    aliases {
        mmc1 = "/mmc@11230000";
        mmc0 = "/mmc@11240000";
    };
    
    reserved-memory {
        #address-cells = <1>;
        #size-cells = <1>;
        ranges;

       /* 128 KiB reserved for ARM Trusted Firmware (BL31) */
        secmon_reserved: secmon@43000000 {
            reg = <0x43000000 0x20000>;
            no-map;
        };
    };
};

